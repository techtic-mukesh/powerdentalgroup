<script>
(function() {

  function moveActivityBlock() {
    
    const transactionsSection = document.querySelector(".transactions-section");
    const rivoActivityBlock = document.querySelector("#rivo-account-dashboard .rivo-account-block .rivo-account-activity-block");
    
    if (!transactionsSection) {
      return;
    }
    
    if (!rivoActivityBlock) {
      return;
    }
    
    // Remove "no-transactions" div
    const noTrans = transactionsSection.querySelector(".no-transactions");
    if (noTrans) {
      noTrans.remove();
    }
    
    // Remove empty rivo-account-activity-block if exists
    const emptyBlock = transactionsSection.querySelector(".rivo-account-activity-block");
    if (emptyBlock) {
      emptyBlock.remove();
    }
    
    // Move the entire Rivo activity block
    transactionsSection.appendChild(rivoActivityBlock);
    
    console.log("Successfully moved rivo activity block");
    
    // Wait longer for Alpine.js to render content
    setTimeout(() => {
      const totalSpent = calculateMinusPoints();
      if (totalSpent >= 0) {
        updateSpentCard(totalSpent);
      }
    }, 2000);
  }

  function calculateMinusPoints() {
    let allRows = document.querySelectorAll("#points-logs-activity tbody tr:not(:has(template))");
    
    if (allRows.length === 0) {
      allRows = document.querySelectorAll("#points-logs-activity tbody tr");
    }
    
    console.log(`Total rows found: ${allRows.length}`);
    
    if (allRows.length === 0) {
      console.log("No rows found in table");
      return 0;
    }
    
    let totalMinusPoints = 0;
    
    allRows.forEach((row, index) => {
      // Skip template rows
      if (row.querySelector('template') || row.querySelector('.rivo-lp-skeleton-loader')) {
        return;
      }
      
      const cells = row.querySelectorAll('td');
      
      if (cells.length >= 2) {
        const pointsCell = cells[1];
        let text = pointsCell.textContent || pointsCell.innerText || '';
        text = text.trim();
        
        // Check for minus
        if (text.includes('-') || text.includes('âˆ’') || text.includes('â€“')) {
          const numbers = text.match(/\d+/);
          
          if (numbers) {
            const points = parseInt(numbers[0]);
            totalMinusPoints += points;
            console.log(`Row ${index + 1}: Found minus ${points} points`);
          }
        }
      }
    });
    
    console.log(`Total spent points: ${totalMinusPoints}`);
    return totalMinusPoints;
  }

  function updateSpentCard(totalSpent) {
    // Find the spent card
    const spentCard = document.querySelector(".point-card.spent");
    
    if (!spentCard) {
      console.log("Spent card not found");
      return;
    }
    
    // Find the h3 element inside point-data
    const h3Element = spentCard.querySelector(".point-data h3");
    
    if (h3Element) {
      // Update the text - remove minus sign
      h3Element.textContent = `${totalSpent} point${totalSpent !== 1 ? 's' : ''}`;
      console.log(`Updated spent card: ${totalSpent} points`);
    } else {
      console.log("h3 element not found in spent card");
    }
  }

  const observer = new MutationObserver((mutations) => {
    const rivoActivityBlock = document.querySelector("#rivo-account-dashboard .rivo-account-block .rivo-account-activity-block");
    const dataRows = document.querySelectorAll("#points-logs-activity tbody tr:not(:has(template)):not(:has(.rivo-lp-skeleton-loader))");
    
    if (rivoActivityBlock && dataRows.length > 0) {
      console.log("Table with data found! Moving now...");
      moveActivityBlock();
      observer.disconnect();
    }
  });

  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', function() {
      const body = document.body;
      if (body) {
        observer.observe(body, { childList: true, subtree: true });
      }
    });
  } else {
    observer.observe(document.body, { childList: true, subtree: true });
  }

  setTimeout(() => {
    console.log("Backup timeout triggered");
    moveActivityBlock();
    observer.disconnect();
  }, 4000);

})();
</script>