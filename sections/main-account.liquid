{{ 'account.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }

  .account-tabs-wrapper {
    display: flex;
    gap: 40px;
    max-width: 1400px;
    margin: 0 auto;
  }

  .account-sidebar ul.account-tabs {
    padding: 0;
}

  .account-sidebar {
    min-width: 320px;
    padding: 15px 0;
    border-radius: 4px;
        background: #fff;
  }

  .account-sidebar h2 {
    font-size: 18px;
    font-weight: 600;
    margin-bottom: 20px;
  }

  .account-tabs {
    list-style: none;
    padding: 0;
    margin: 0;
  }



  .account-tabs button,   .account-tabs a {
   width: 100%;
    text-align: left;
    background: unset;
    border: none;
    cursor: pointer;
    font-size: 16px;
    border-radius: 0;
    transition: all 0.2s;
    color: #000;
    margin: 0;
    padding: 5px 18px 5px 15px;
    justify-content: flex-start;
    font-family: 'Montserrat';
    font-weight: 500;
    letter-spacing: -.16px;
     border-left: 3px solid transparent;
     text-decoration: none;
         display: flex !important;
    height: 42px;
    align-items: center;
  }

  .account-tabs button, .account-tabs a {
    color: #01000A;
    font-family: Prompt;
    font-size: 16px;
    font-style: normal;
    font-weight: 300;
    line-height: 22px; /* 137.5% */
    text-transform: capitalize !important;
        display: flex;
    height: 42px;
    align-items: center;
}

  .account-tabs button a {
    padding-left: 0;
}

  .account-tabs button:hover {
      border-color:  #42C5EF;
       opacity:1;
        background: linear-gradient(90deg, #42C5EF 0%, #0033A0 100%);
background-clip: text;
-webkit-background-clip: text;
-webkit-text-fill-color: transparent;
  }

   .account-tabs button:hover  svg path, .account-tabs  a:hover  svg path{
    fill: linear-gradient(90deg, #42C5EF 0%, #0033A0 100%);
stroke-width: 0.3px;
stroke: #42C5EF;
}

   .account-tabs  a:hover {
border-color:  #42C5EF;
       opacity:1;
       background: linear-gradient(90deg, #42C5EF 0%, #0033A0 100%);
background-clip: text;
-webkit-background-clip: text;
-webkit-text-fill-color: transparent;
    }

  .account-tabs button.active {
         border-color:  #42C5EF;
           background: linear-gradient(90deg, #42C5EF 0%, #0033A0 100%);
background-clip: text;
-webkit-background-clip: text;
-webkit-text-fill-color: transparent;
  }

  .account-content {
    flex: 1;
    min-width: 0;
  }

  .tab-content {
    display: none;
  }

  .tab-content.active {
    display: block;
  }

  .points-header {
    margin-bottom: 40px;
  }

  .points-header h1 {
    font-size: 42px;
    font-weight: 700;
    margin-bottom: 30px;
    text-transform: uppercase;
  }

  .points-cards {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 20px;
    margin-bottom: 50px;
  }

.point-card {
    padding: 25px;
    border-radius: 25px;
    background: #FFF !important;
    display: flex;
    align-items: center;
    gap: 35px;
}
 

 .point-card .point-data {
    display: flex;
    flex-direction: column;
}

.point-card .point-data p {
    color: #01000A;
    font-family: Prompt;
    font-size: 16px;
    font-style: normal;
    font-weight: 300;
    line-height: 22px; /* 137.5% */
    margin: 0 0 7px;
}

.point-card .point-data h3 {
    color: #01000A;
    font-family: Montserrat;
    font-size: 24px;
    font-style: normal;
    font-weight: 700;
    line-height: 24px; /* 100% */
    text-transform: uppercase;
    margin: 0;
    text-align: left;
}


  .transactions-section h2 {
     color: #01000A;
    font-family: Montserrat;
    font-size: 24px;
    font-style: normal;
    font-weight: 700;
    line-height: 24px; /* 100% */
    text-transform: uppercase;
    margin: 0 0 20px;
  }

  .no-transactions {
    background: #fff3cd;
    border: 1px solid #ffc107;
    padding: 16px 20px;
    border-radius: 4px;
    display: flex;
    align-items: center;
    gap: 12px;
  }

  .no-transactions::before {
    content: "âš ";
    font-size: 20px;
    color: #856404;
  }

  .not-available {
    text-align: center;
    padding: 60px 20px;
    color: #666;
  }

  .not-available h3 {
    font-size: 24px;
    margin-bottom: 10px;
  }

  @media screen and (max-width: 990px) {
    .account-tabs-wrapper {
      flex-direction: column;
    }

    .account-sidebar {
      min-width: auto;
    }

    .points-header h1 {
      font-size: 32px;
    }
  }
{%- endstyle -%}

