    <main class="account-content">
      <!-- My Account Tab -->
      <div class="tab-content active" id="account-tab">
        {% comment %} <h1>{{ 'customer.account.title' | t }}</h1> {% endcomment %}
        <div class="ac-info">
  <h2 style="margin-bottom: 5px;">Hi, {{ customer.first_name }}!</h2>
  <p style="margin: 0 0 20px;">Welcome to your account dashboard.</p>

  <h3 style="margin-bottom: 10px;">{{ 'customer.account.details' | t }}</h3>
  {{ customer.default_address | format_address }}

  <a href="{{ routes.account_addresses_url }}" style="display: inline-block; margin-top: 15px;">
    {{ 'customer.account.view_addresses' | t }} ({{ customer.addresses_count }})
  </a>
</div>

      </div>

      <!-- My Orders Tab -->
      <div class="tab-content" id="orders-tab">
        <h1>{{ 'customer.orders.title' | t }}</h1>
        {% paginate customer.orders by 20 %}
          {%- if customer.orders.size > 0 -%}
            <table role="table" class="order-history" style="width: 100%; margin-top: 30px;">
              <thead>
                <tr>
                  <th>{{ 'customer.orders.order_number' | t }}</th>
                  <th>{{ 'customer.orders.date' | t }}</th>
                  <th>{{ 'customer.orders.payment_status' | t }}</th>
                  <th>{{ 'customer.orders.fulfillment_status' | t }}</th>
                  <th>{{ 'customer.orders.total' | t }}</th>
                </tr>
              </thead>
              <tbody>
                {%- for order in customer.orders -%}
                  <tr>
                    <td><a href="{{ order.customer_url }}">{{ order.name }}</a></td>
                    <td>{{ order.created_at | time_tag: format: 'date' }}</td>
                    <td>{{ order.financial_status_label }}</td>
                    <td>{{ order.fulfillment_status_label }}</td>
                    <td>{{ order.total_net_amount | money_with_currency }}</td>
                  </tr>
                {%- endfor -%}
              </tbody>
            </table>
          {%- else -%}
            <p style="margin-top: 30px;">{{ 'customer.orders.none' | t }}</p>
          {%- endif -%}
        {% endpaginate %}
      </div>

      <!-- Address Book Tab -->
      <div class="tab-content" id="address-tab">
        <div class="not-available">
          <h3>Address Book</h3>
          <p>This feature is currently not available.</p>
        </div>
      </div>

      <!-- Account Information Tab -->
      <div class="tab-content" id="info-tab">
        <div class="not-available">
          <h3>Account Information</h3>
          <p>This feature is currently not available.</p>
        </div>
      </div>

      <!-- My Points and Rewards Tab -->
